{
  "name": "orbis-mstock",
  "version": "1.0.0",
  "description": "",
  "main": "server/_app/index.js",
  "scripts": {
    "babel-node": "cross-env BABEL_ENV=development cross-env NODE_PATH=server babel-node .",
    "prebuild": "npm run clean",
    "prebuild:cordova": "npm run clean",
    "build:dev": "cross-env NODE_ENV=development cross-env BABEL_ENV=development webpack",
    "build": "cross-env NODE_ENV=production cross-env BABEL_ENV=production webpack -p",
    "build:cordova:dev": "cross-env NODE_ENV=development cross-env BABEL_ENV=development cross-env CORDOVA=true webpack",
    "build:cordova": "cross-env NODE_ENV=production cross-env cross-env BABEL_ENV=production CORDOVA=true webpack --progress",
    "build:cordova:dev:win": ".\\scripts\\build-and-install-android-dev.bat",
    "start": "npm run babel-node",
    "clean": "rimraf dist/*",
    "clean:cordova": "rimraf dist/cordova/*",
    "lint": "cross-env NODE_PATH=server xo",
    "pretest": "#npm run lint",
    "test": "cross-env BABEL_ENV=test jest",
    "test:watch": "cross-env BABEL_ENV=test jest --watch",
    "test:coverage": "cross-env BABEL_ENV=test nyc --reporter=lcov jest --coverage",
    "prestart:server:dev": "npm t",
    "start:server:dev": "npm start",
    "start:client:dev": "cross-env BABEL_ENV=development cross-env NODE_ENV=development babel-node webpack.dev-server.js",
    "watch:server": "cross-env NODE_ENV=development nodemon -w server -w package.json -x npm run start:server:dev",
    "watch:client": "cross-env BABEL_ENV=development cross-env NODE_ENV=development nodemon -w webpack.web.js -w webpack.dev-server.js -x npm run start:client:dev",
    "webpack-dashboard": "webpack-dashboard -- npm run watch:client",
    "postwatch:client": "npm run tunnel",
    "cordova:init": "cordova plugin add cordova-plugin-device; cordova plugin add cordova-plugin-whitelist",
    "precordova:link": "rimraf cordova/www/*",
    "cordova:link": "shx cp -R dist/cordova/* cordova/www/",
    "precordova:emulate:ios": "npm run cordova:link",
    "cordova:emulate:ios": "cd cordova && cordova emulate ios",
    "precordova:emulate:android": "npm run cordova:link",
    "cordova:emulate:android": "cd cordova && cordova emulate android",
    "tunnel": "lt --port 4000 --subdomain obms",
    "api-mock": "api-mock --port 3333 api-blueprint/spec.md",
    "watch:cordova:browser:dev": "nodemon -w client -w webpack.cordova.js -w webpack.dev-server.js -x 'cross-env NODE_TARGET=browser npm run build:cordova:dev && npm run cordova:link && cd cordova && cordova run browser'",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "bluekit": "bluekit --baseDir ./client/components --paths . --exclude \"./(Layout|StyledComponent).tsx\"",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "prepare-release-notes": "cat ./release-notes/* > ./RELEASE.md",
    "preview-release-notes": "npm run prepare-release-notes && markdownb ./RELEASE.md",
    "build-release-notes": "npm run prepare-release-notes && markdown ./RELEASE.md > RELEASE.html",
    "cypress:open": "cypress open"
  },
  "keywords": [],
  "author": "Lee Driscoll <lee.driscoll@orbistech.co.uk>",
  "license": "ISC",
  "devDependencies": {
    "@storybook/react": "^3.3.14",
    "autobind-decorator": "^2.1.0",
    "ava": "^0.18.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "7",
    "babel-jest": "^22.4.0",
    "babel-loader": "^7.1.2",
    "babel-minify-webpack-plugin": "^0.2.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-module-resolver": "^3.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-classes": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-react-jsx-source": "^6.22.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-loose": "^8.0.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "babelify": "^8.0.0",
    "body-parser": "^1.17.1",
    "bunyan": "^1.8.8",
    "chalk": "^2.3.1",
    "compression-webpack-plugin": "^1.0.1",
    "connected-react-router": "^4.2.1",
    "cordova": "^8.0.0",
    "core-decorators": "^0.18.0",
    "cross-env": "^3.2.4",
    "css-loader": "^0.27.3",
    "cypress": "^3.0.0",
    "debug": "^2.6.2",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.1",
    "eslint": "^4.17.0",
    "eslint-config-xo": "^0.20.1",
    "eslint-config-xo-react": "^0.11.1",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^7.7.0",
    "express": "^4.15.2",
    "faker": "^4.1.0",
    "fetch-mock": "^6.0.1",
    "file-loader": "^0.11.1",
    "grommet": "^1.10.1",
    "grommet-addons": "^0.6.0",
    "gulp-webpack": "^1.5.0",
    "history": "^4.6.1",
    "html-loader": "^0.5.1",
    "html-webpack-cdn-plugin": "^0.2.0",
    "html-webpack-plugin": "^2.28.0",
    "identity-obj-proxy": "^3.0.0",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^22.4.0",
    "json-loader": "^0.5.4",
    "lnk-cli": "^1.0.1",
    "localforage": "^1.5.6",
    "localforage-cordovasqlitedriver": "^1.6.0",
    "lodash": "^4.17.4",
    "nock": "^9.0.11",
    "node-sass": "^4.9.0",
    "nodemon": "^1.11.0",
    "nyc": "^11.4.1",
    "postcss-loader": "^1.3.3",
    "pouch-redux-middleware": "^0.6.1",
    "pouchdb": "^6.2.0",
    "progress-promise": "^0.0.6",
    "prop-types": "^15.6.0",
    "quagga": "^0.11.6",
    "ramda": "^0.25.0",
    "re-reselect": "^1.0.1",
    "react": "^16.2.0",
    "react-detect-offline": "^2.0.1",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-redux": "^5.0.4",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.6",
    "react-test-renderer": "^16.2.0",
    "react-toggle": "^4.0.2",
    "redux": "^3.6.0",
    "redux-actions": "^2.2.1",
    "redux-devtools": "^3.3.2",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-extension": "^2.13.0",
    "redux-devtools-log-monitor": "^1.2.0",
    "redux-logger": "^3.0.1",
    "redux-mock-store": "^1.5.1",
    "redux-persist": "^5.5.0",
    "redux-persist-transform-filter": "^0.0.16",
    "redux-test-utils": "^0.2.2",
    "redux-thunk": "^2.2.0",
    "regenerator-runtime": "^0.11.1",
    "reselect": "^3.0.1",
    "reselect-map": "^1.0.3",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.3",
    "shelljs": "^0.7.7",
    "shx": "^0.2.2",
    "stacktrace-js": "^2.0.0",
    "storybook": "0.0.0",
    "style-loader": "^0.16.0",
    "uuid": "^3.0.1",
    "valid-url": "^1.0.9",
    "webpack": "^2.0.0",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-hot-middleware": "^2.17.1",
    "xo": "^0.18.2"
  },
  "jest": {
    "globals": {
      "BABEL_ENV": "test"
    },
    "setupFiles": [
      "./client/test/jest-setup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "identity-obj-proxy",
      "^~(.*)$": "<rootDir>/client/$1",
      "^~features(.*)$": "<rootDir>/client/features/$1",
      "^~components(.*)$": "<rootDir>/client/components/$1"
    },
    "transform": {
      "^.+\\.jsx$": "babel-jest",
      "^.+\\.js$": "babel-jest"
    },
    "collectCoverageFrom": [
      "{client,server}/**/*.{js,jsx}",
      "!**/__fixtures/**"
    ]
  },
  "nyc": {
    "exclude": [
      "coverage/**",
      "test/**",
      "test{,-*}.js",
      "**/*.test.js",
      "**/__tests__/**",
      "**/node_modules/**",
      "**/__fixtures__/**"
    ]
  },
  "ava": {
    "require": "babel-register",
    "babel": "inherit"
  },
  "eslintConfig": {
    "plugins": [
      "ramda"
    ],
    "extends": [
      "react-app",
      "plugin:ramda/recommended"
    ]
  },
  "xo": {
    "parser": "babel-eslint",
    "space": true,
    "semicolon": false,
    "rules": {
      "no-prototype-builtins": 0,
      "object-curly-spacing": [
        "error",
        "always",
        {
          "objectsInObjects": false
        }
      ]
    },
    "overrides": [
      {
        "files": "server/**/*.js",
        "settings": {
          "import/resolver": {
            "node": {
              "paths": [
                "server"
              ]
            }
          }
        }
      },
      {
        "extends": [
          "xo",
          "xo-react"
        ],
        "files": "client/**/*.js{,x}",
        "envs": [
          "browser"
        ],
        "rules": {
          "unicorn/filename-case": 0
        }
      }
    ]
  },
  "dependencies": {}
}
